DROP TABLE IF EXISTS `product_line`;
CREATE TABLE `product_line` (
  `Id` int(11) NOT NULL AUTO_INCREMENT,
  `name` varchar(255) DEFAULT 'line1' COMMENT '产品线名',
  `mangerId` int(11) NOT NULL DEFAULT '0' COMMENT '产品线负责人',
  `line_bes` varchar(255) DEFAULT NULL COMMENT '产品线说明',
  PRIMARY KEY (`Id`),
  KEY `mangerId` (`mangerId`),
  CONSTRAINT `product_line_ibfk_1` FOREIGN KEY (`mangerId`) REFERENCES `user` (`user_id`)
) ENGINE=InnoDB AUTO_INCREMENT=5 DEFAULT CHARSET=utf8 COMMENT='产品线';

