#
# Structure for table "change_task"
#

DROP TABLE IF EXISTS `change_task`;
CREATE TABLE `change_task` (
  `change_id` int(11) NOT NULL AUTO_INCREMENT COMMENT '主键变更条目',
  `change_item` int(11) NOT NULL DEFAULT '0' COMMENT '变更类型，0变跟时间，1变更进度，2任务扩展,3恢复挂起的任务',
  `change_content` varchar(255) DEFAULT NULL COMMENT '变更内容',
  `change_time` datetime NOT NULL DEFAULT '0000-00-00 00:00:00' COMMENT '变更时间申请时间',
  `change_reason` varchar(255) NOT NULL DEFAULT '' COMMENT '变更原因',
  `change_taskid` int(11) DEFAULT NULL COMMENT '变更关联任务id，外键',
  `change_bakId` int(11) DEFAULT NULL COMMENT '任务变更前的任务的信息。包含时间的变化等。为空则为任务扩展。',
  `review_content` varchar(255) NOT NULL DEFAULT '不同意' COMMENT '评审人意见',
  `general_manager_review` varchar(255) NOT NULL DEFAULT '不同意' COMMENT '副总意见',
  `general_manager_id` int(11) DEFAULT NULL COMMENT '研发副总ID',
  `auditor_id` int(11) DEFAULT NULL COMMENT '审核人Id',
  `audior_date` datetime NOT NULL DEFAULT '0000-00-00 00:00:00' COMMENT '部门经理审核日期',
  `general_date` datetime NOT NULL DEFAULT '0000-00-00 00:00:00' COMMENT '副总审核日期',
  `status` int(11) DEFAULT '0' COMMENT '变更结果',
  PRIMARY KEY (`change_id`),
  KEY `change_taskid` (`change_taskid`),
  KEY `general_manager_id` (`general_manager_id`),
  KEY `auditor_id` (`auditor_id`),
  KEY `change_bakId` (`change_bakId`),
  CONSTRAINT `change_task_ibfk_1` FOREIGN KEY (`change_taskid`) REFERENCES `task` (`task_id`),
  CONSTRAINT `change_task_ibfk_2` FOREIGN KEY (`change_bakId`) REFERENCES `task` (`task_id`),
  CONSTRAINT `change_task_ibfk_3` FOREIGN KEY (`general_manager_id`) REFERENCES `user` (`user_id`),
  CONSTRAINT `change_task_ibfk_4` FOREIGN KEY (`auditor_id`) REFERENCES `user` (`user_id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='变更';

