#
# Structure for table "task"
#

DROP TABLE IF EXISTS `task`;
CREATE TABLE `task` (
  `task_id` int(11) NOT NULL AUTO_INCREMENT,
  `task_attri` int(11) NOT NULL DEFAULT '-1' COMMENT '任务属性，-1:正常任务， 其他值：备份任务',
  `task_name` varchar(255) DEFAULT NULL COMMENT '任务名',
  `task_des` varchar(255) DEFAULT NULL COMMENT '任务描述,包括任务审核意见',
  `task_ownerid` int(11) DEFAULT '3' COMMENT '负责人，外键',
  `task_project_id` int(11) DEFAULT NULL COMMENT '所属项目',
  `task_plan_starttime` date DEFAULT '0000-00-00' COMMENT '计划开始时间',
  `task_plan_closetime` date DEFAULT NULL COMMENT '计划完成时间',
  `task_plan_period` float NOT NULL DEFAULT '0' COMMENT '计划工期',
  `task_ac_starttime` date DEFAULT NULL COMMENT '实际开始，在未开始变时，此值可设置。',
  `task_ac_closetime` date DEFAULT NULL COMMENT '实际完成时间，即提交完成的时间',
  `task_ac_period` float NOT NULL DEFAULT '0' COMMENT '实际工期',
  `task_type` varchar(255) NOT NULL DEFAULT '' COMMENT '任务类型，项目任务，部门任务',
  `task_score` int(11) NOT NULL DEFAULT '0' COMMENT '主管对任务的评分',
  `forecast_task_score` int(11) NOT NULL DEFAULT '100' COMMENT '系统预计分数值',
  `task_score_des` varchar(255) DEFAULT '' COMMENT '评分说明',
  `task_auditorid` int(11) DEFAULT '-1' COMMENT '评审人，外键',
  `task_review_date` datetime DEFAULT '0000-00-00 00:00:00' COMMENT '任务发布评审时间',
  `task_score_date` datetime DEFAULT '0000-00-00 00:00:00' COMMENT '任务评分时间',
  `task_statusid` int(11) NOT NULL DEFAULT '1' COMMENT '任务状态，外键',
  `task_dif` float(16,0) NOT NULL DEFAULT '0' COMMENT '任务难度系数，任务发布评审时必须填此值，范围（1.0~3.0）',
  `task_createTime` datetime DEFAULT '0000-00-00 00:00:00' COMMENT '任务创建时间，若为备份任务，则为备份时间',
  `task_timeType_id` int(11) DEFAULT '1' COMMENT '工时类型',
  `creatorid` int(11) NOT NULL DEFAULT '3',
  PRIMARY KEY (`task_id`),
  KEY `task_ownerid` (`task_ownerid`),
  KEY `task_ibfk_2` (`task_project_id`),
  KEY `task_auditorid` (`task_auditorid`),
  KEY `task_statusid` (`task_statusid`),
  KEY `task_timeType_id` (`task_timeType_id`),
  KEY `creatorid` (`creatorid`),
  CONSTRAINT `task_ibfk_1` FOREIGN KEY (`task_ownerid`) REFERENCES `user` (`user_id`),
  CONSTRAINT `task_ibfk_2` FOREIGN KEY (`task_auditorid`) REFERENCES `user` (`user_id`),
  CONSTRAINT `task_ibfk_3` FOREIGN KEY (`task_timeType_id`) REFERENCES `timetype` (`id`),
  CONSTRAINT `task_ibfk_4` FOREIGN KEY (`creatorid`) REFERENCES `user` (`user_id`)
) ENGINE=InnoDB AUTO_INCREMENT=400 DEFAULT CHARSET=utf8;

