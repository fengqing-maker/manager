DROP TABLE IF EXISTS `project`;
CREATE TABLE `project` (
  `project_id` int(11) NOT NULL AUTO_INCREMENT,
  `project_name` varchar(255) DEFAULT '' COMMENT '项目名称',
  `project_managerid` int(11) NOT NULL DEFAULT '0' COMMENT '项目负责人',
  `line_id` int(11) DEFAULT '0' COMMENT '所属产品线',
  `project_shortName` varchar(255) DEFAULT NULL COMMENT '项目简称',
  PRIMARY KEY (`project_id`),
  KEY `project_managerid` (`project_managerid`),
  KEY `project_ibfk_2` (`line_id`),
  CONSTRAINT `project_ibfk_2` FOREIGN KEY (`line_id`) REFERENCES `product_line` (`Id`),
  CONSTRAINT `project_ibfk_3` FOREIGN KEY (`project_managerid`) REFERENCES `user` (`user_id`)
) ENGINE=InnoDB AUTO_INCREMENT=30 DEFAULT CHARSET=utf8;

